table Medidas
	lineageTag: d29aad8c-b158-4c1f-bf5e-e4c4b1ea5853

	measure 'Qtd Funcionarios' = ```
			
			CALCULATE(
			    COUNT(fact_Colaboradores[COLABORADORES]),
			    TREATAS(VALUES(aux_Diretoria[MesAno]), fact_Colaboradores[MesAno]),
			    TREATAS(VALUES(aux_Diretoria[DIRETORIA]), fact_Colaboradores[DIRETORIA]),
			    TREATAS(VALUES(aux_Time[TIME]), fact_Colaboradores[TIME])
			)
			
			```
		formatString: 0
		lineageTag: 720e92f7-45a4-425a-bccc-76a41c4312d9

	measure 'Pct Colaboradores Diretoria' = ```
			
			VAR _Qtd = [Qtd Funcionarios]
			VAR _Total =
			    CALCULATE(
			        COUNT(fact_Colaboradores[COLABORADORES]),
			        ALL(fact_Colaboradores[DIRETORIA])
			    )
			RETURN
			    IF(
			        SELECTEDVALUE(aux_Diretoria[DIRETORIA]) = "TOTAL",
			        1,                                   -- 100%
			        DIVIDE(_Qtd, _Total)
			    )
			
			```
		formatString: 0.00%;-0.00%;0.00%
		lineageTag: facf9b43-632c-4d89-8e0a-2718fd7dd12f

	measure 'Totalizador Diretoria' = ```
			
			VAR _Colaboradores = CALCULATE( [Qtd Funcionarios] )
			VAR _Total =
			IF(
			    SELECTEDVALUE( aux_Diretoria[DIRETORIA] ) = "TOTAL",
			    CALCULATE( 
			        COUNT( fact_Colaboradores[COLABORADORES] ),
			        ALL( fact_Colaboradores )
			    )
			)
			RETURN
			    _Colaboradores + _Total
			```
		formatString: 0
		lineageTag: 5bf2c2f6-e313-4afc-8038-a5310d1c6ecb

	measure 'Eixo Ordinal Diretoria' = ```
			
			SWITCH(
			    TRUE(), 
			    SELECTEDVALUE(aux_Diretoria[DIRETORIA]) IN VALUES( fact_Colaboradores[DIRETORIA] ),
			    RANK(
			        DENSE,
			        ALL(aux_Diretoria[DIRETORIA]),
			        ORDERBY([Qtd Funcionarios], ASC)
			    ) +9
			)
			```
		formatString: 0
		lineageTag: cc3972e4-eac0-48ad-983e-19adc694b555

